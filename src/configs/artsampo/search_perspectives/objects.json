        "prefixesFile": "SparqlQueriesPrefixes.js"
    },
    "sparqlQueriesFile": "SparqlQueriesObjects.js",
    "baseURI": "http://ldf.fi/artsampo",
    "URITemplate": "<BASE_URI>/fng/<LOCAL_ID>",
    "facetClass": ":Object",
    "frontPageImage": "main_page/sampo.jpg",
